  "bugs": {
    "url": "https://github.com/iPan7/googlebooksreactsearchhw/issues"
  },
  "homepage": "https://github.com/iPan7/googlebooksreactsearchhw#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.2",
    "react-router-dom": "^5.2.0"
  }
}
